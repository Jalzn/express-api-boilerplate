{
  "name": "express-api-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "build": "npx tsc && npx tsc-alias",
    "serve": "concurrently \"npx tsc -w\" \"npx tsc-alias -w\" \"nodemon --inspect ./dist/index.js\"",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-error": "^0.0.6",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.1",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  }
}
